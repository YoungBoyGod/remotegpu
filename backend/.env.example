# RemoteGPU 后端环境变量配置模板
# 复制此文件为 .env 并修改为实际值
# cp .env.example .env

# 数据库
DB_PASSWORD=changeme_db_password

# Redis
REDIS_PASSWORD=changeme_redis_password

# JWT 认证（至少32字符的随机字符串）
JWT_SECRET=changeme_jwt_secret_at_least_32_chars

# AES-256 加密密钥（恰好32字节）
AES_KEY=changeme_aes_key_exactly_32bytes

# S3/RustFS 对象存储
S3_ACCESS_KEY=changeme_s3_access_key
S3_SECRET_KEY=changeme_s3_secret_key

# Harbor 镜像仓库
HARBOR_PASSWORD=changeme_harbor_password

# JumpServer 堡垒机
JUMPSERVER_API_KEY=changeme_jumpserver_api_key

# Guacamole 远程桌面
GUACAMOLE_USER=guacadmin
GUACAMOLE_PASSWORD=changeme_guacamole_password

# 邮件服务
MAIL_PASSWORD=changeme_mail_password

# Agent 认证 Token
AGENT_TOKEN=changeme_agent_token
